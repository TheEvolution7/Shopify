<!--====== Back to Top ======-->
<a href="#" class="back-to-top" id="backToTop">
  <i class="fal fa-angle-double-up"></i>
</a>
<!--====== FOOTER PART START ======-->
<footer class="footer-two">
  <div class="footer-widget-area">
    <div class="container">
      <div class="row">
        {%- for block in section.blocks -%}
        {%- case block.type -%}
        {%- when 'text' -%}
        <div class="col-lg-3 col-sm-6 order-1">
          <!-- Site Info Widget -->
          <div class="widget site-info-widget mb-30">
            <div class="footer-logo mb-30">
              <img src="{{ section.settings.logo | img_url: 'master' }}">
            </div>
            <p>
              {{ block.settings.text }}
            </p>
            <div class="social-links mt-40">
              <a href="#"><i class="fab fa-facebook-f"></i></a>
              <a href="#"><i class="fab fa-youtube"></i></a>
            </div>
          </div>
        </div>
        {%- when 'link_contact' -%}
        <div class="col-lg-3 col-sm-6 order-2 order-lg-3">
          <!-- Contact Widget -->
          <div class="widget contact-widget mb-30">
            <h4 class="widget-title">Contact Us.</h4>
            <div class="contact-lists">
              <div class="contact-box">
                <div class="icon">
                  <i class="flaticon-call"></i>
                </div>
                <div class="desc">
                  <h6 class="title">Phone Number</h6>
                  <a href="tel:+123456789">+123 456789</a>
                </div>
              </div>
              <div class="contact-box">
                <div class="icon">
                  <i class="flaticon-message"></i>
                </div>
                <div class="desc">
                  <h6 class="title">Email Address</h6>
                  <a href="mailto:info@albi.com">info@albi.com</a>
                </div>
              </div>
              <div class="contact-box">
                <div class="icon">
                  <i class="flaticon-location-pin"></i>
                </div>
                <div class="desc">
                  <h6 class="title">Office Address</h6>
                  123, Miranda City, NYC
                </div>
              </div>
            </div>
          </div>
        </div>
        {%- when 'link_contact' -%}
        <div class="col-lg-6 order-3 order-lg-2">
          <!-- Nav Widget -->
          <div class="widget nav-widget mb-30">
            <div>
              <h4 class="widget-title">Our Page</h4>
              <ul>
                <li><a href="/pages/about">Our Story</a></li>
                <li><a href="/collections">Product Page</a></li>
                <li><a href="/pages/warranty">Warranty</a></li>
                <li><a href="#">Financing</a></li>
                <li><a href="#">Dimensions chart</a></li>
                <li><a href="#">Returns and Refunds</a></li>
                <li><a href="#">We Give Back</a></li>
                <li><a href="#">Affiliate Program</a></li>
                <li><a href="/pages/contact">Contact Us</a></li>
              </ul>
            </div>
          </div>
        </div>
        {%- endcase -%}
        {% endfor %}

      </div>
    </div>
  </div>
  <div class="copyright-area pt-30 pb-30">
    <div class="container">
      <div class="row align-items-center">
        <div class="col-lg-6 col-md-5 order-2 order-md-1">
          <p class="copyright-text copyright-two">Albi - 2021</p>
        </div>
        <div class="col-lg-6 col-md-7 order-1 order-md-2">
          <div class="footer-menu text-center text-md-right">
            <ul>
              <li><a href="/policies/terms-of-service">Terms of use</a></li>
              <li><a href="/policies/privacy-policy">Privacy Environmental Policy</a></li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
</footer>
<!--====== FOOTER PART END ======-->

