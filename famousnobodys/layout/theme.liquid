        function login() {
          $('#page-lost-password').hide();
          $('#move-to-lost').on('click',function() {
            $('#page-lost-password').show();
            $('#page-login').hide();
          })
          $('#back-login').on('click',function() {
            $('#page-lost-password').hide();
            $('#page-login').show();
          })
        }
        login();

      })();
