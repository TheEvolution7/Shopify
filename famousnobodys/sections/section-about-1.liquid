<header class="s-header-text">
            <div class="s-header-heading" data-effect="heading">
               <h3 class="f-st">{{ section.settings.heading }}</h3>
                <h2>{{ section.settings.text }}</h2>
            </div> <button class="s-header-scroll">Scroll<svg viewBox="0 0 6 10">
                    <path
                        d="M0,9.9C1.4,8.3,2.9,5.6,3.4,5C2.9,4.4,1.4,1.7,0,0.1L0.1,0C1.5,1.6,5.8,4.8,6,5 c-0.2,0.2-4.5,3.4-5.9,5L0,9.9z" />
                </svg> <svg viewBox="0 0 6 10">
                    <path
                        d="M0,9.9C1.4,8.3,2.9,5.6,3.4,5C2.9,4.4,1.4,1.7,0,0.1L0.1,0C1.5,1.6,5.8,4.8,6,5 c-0.2,0.2-4.5,3.4-5.9,5L0,9.9z" />
                </svg> </button>
            <div class="s-header-background" data-background="#f6f4f2"></div>
</header>
	<section class="s-banner">
            <div class="s-banner-container a-left">
                <div class="s-banner-text" data-effect="block">
                    <h3>{{ section.settings.heading-2 }}</h3>
                    <h2> {{ section.settings.title-1 }}</h2>
                  <p>{{ section.settings.des }}</p>
                </div>
                <div class="s-banner-figure">
                    <div class="s-banner-src"> <img src="{{ section.settings.image-1 | img_url: '500x' }}"
                            alt="{{ section.settings.image-1.alt }}"> </div>
                    <div class="s-banner-src"> <img
                            src="{{ section.settings.image-2 | img_url: '500x' }}"
                            alt=" {{ section.settings.image-2.alt }}"> </div>
                </div>
                <div class="s-banner-background" data-background="#f6f4f2"></div>
            </div>
        </section>

{% schema %}
{
  "name": "Section About 1",
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label": "Heading 1",
      "default": "Latest Collection"
    },
	{
      "type": "text",
      "id": "text",
      "label": "Title",
      "default": "Title"
    },
  	{
      "type": "text",
      "id": "heading-2",
      "label": "Heading 2",
      "default": "Values"
    },
 	{
      "type": "text",
      "id": "title-1",
      "label": "Title 2",
      "default": "Title"
    },
	{
      "type": "text",
      "id": "des",
      "label": "Description",
      "default": "The Anti - The Revolution. Empowerment. Tragedy Triumphs. The Meltdown Dwells. Movement Conquers. The Meteoric Rise. Fame In Flames. We The People Are All NOBODYS."
    },
	{
       "type": "image_picker",
       "id": "image-1",
       "label": "Image 1"
    },
	{
       "type": "image_picker",
       "id": "image-2",
       "label": "Image 2"
     }
  ] 
}
{% endschema %}

