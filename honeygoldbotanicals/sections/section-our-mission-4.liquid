<section class="clients-section clients-section-bg">
            <div class="auto-container">
                <div class="auto-container">
                    <div class="sec-clin wow fadeInUp" data-wow-duration="3s">
                        <h2>{{ section.settings.title }}</h2>
                    </div>
                    <div class="clients-carousel owl-carousel owl-theme owl-nav-none owl-dots-none">
                      {%- for block in section.blocks -%}
                        <figure class="clients-logo-box"><a href=" {{ block.settings.link }}"><img src="{{ block.settings.image | img_url : '150x' }}" alt="{{ block.settings.image.alt }}"></a></figure>
                      {%- endfor -%} 
                    </div>
                </div>
            </div>
        </section>
{% schema %}
  {
    "name": "Section Our Mission 4",
    "settings": [
		{
          "type": "text",
          "id": "title",
          "label": "Title",
		  "default" : "Partner"
    	}
		],
"blocks": [
    {
      "type": "logo_image",
      "name": "Logo",
      "settings": [
  		{
          "type": "image_picker",
          "id": "image",
          "label": "Logo"
        },
		{
          "type": "url",
          "id": "link",
          "label": "Link",
          "info": "Optional"
        }
      ]
    }
  ]
  }
{% endschema %}