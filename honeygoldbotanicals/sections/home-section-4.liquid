<section class="about-section" style="background-image: url({{ 'banerrr.png' | asset_url }});">
  <div class="auto-container">
    <style>
      .sw-instagram-container .sw-instagram-row {
        padding: 0 50px
      }
      .sw-instagram-container .sw-instagram-nav.nav-prev{
        left: 0!important;
      }
      .sw-instagram-container .sw-instagram-nav.nav-next{
        right: 0!important
      }
    </style>
    <div class="row align-items-center clearfix">
      <div class="col-lg-12 col-md-12 col-sm-12">
        <div class="sec-title centred wow fadeInUp" data-wow-duration="3s">
          <h2>{{ section.settings.text }}</h2>
        </div>
      </div>
    </div>
  </div>
</section>
{% schema %}
  {
    "name": "Section Home 4",
    "settings": [
		{
          "type": "text",
          "id": "text",
          "label": "Title"
    	}
		]
  }
{% endschema %}
