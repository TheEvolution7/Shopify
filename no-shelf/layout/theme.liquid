le-nav--open .icon-close{display:none}.main-content{opacity:0}.main-content .shopify-section{display:none}.main-content .shopify-section:first-child{display:inherit}.critical-hidden{display:none}</style>
  <link rel="stylesheet" href="{{ 'preloader.css' | asset_url }}">
  <link rel="stylesheet" href="{{ 'bootstrap.min.css' | asset_url }}">
  <link rel="stylesheet" href="{{ 'slick.css' | asset_url }}">
  <link rel="stylesheet" href="{{ 'metisMenu.css' | asset_url }}">
  <link rel="stylesheet" href="{{ 'owl.carousel.min.css' | asset_url }}">
  <link rel="stylesheet" href="{{ 'animate.min.css' | asset_url }}">
  <link rel="stylesheet" href="{{ 'jquery.fancybox.min.css' | asset_url }}">
  <link rel="stylesheet" href="{{ 'fontAwesome5Pro.css' | asset_url }}">
  <link rel="stylesheet" href="{{ 'ionicons.min.css' | asset_url }}">
  <link rel="stylesheet" href="{{ 'default.css' | asset_url }}">
  <link rel="stylesheet" href="{{ 'style.css' | asset_url }}">
  {{ content_for_header }}
</head>

<body>
  {% section 'header' %}
  
  {% if request.path == '/challenge' %}
  	{% include 'challenge' %}
  {% else %}
  	{{ content_for_layout }}
  {% endif %}

  

  {% section 'footer' %}  
  
  
  <script src="{{ 'modernizr-3.5.0.min.js' | asset_url }}"></script>
  <script src="{{ 'jquery-3.5.1.min.js' | asset_url }}"></script>
  <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js" integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN" crossorigin="anonymous"></script>
  <script src="{{ 'waypoints.min.js' | asset_url }}"></script>
  <script src="{{ 'bootstrap.bundle.min.js' | asset_url }}"></script>
  <script src="{{ 'metisMenu.min.js' | asset_url }}"></script>
  <script src="{{ 'slick.min.js' | asset_url }}"></script>
  <script src="{{ 'jquery.fancybox.min.js' | asset_url }}"></script>
  <script src="{{ 'isotope.pkgd.min.js' | asset_url }}"></script>
  <script src="{{ 'owl.carousel.min.js' | asset_url }}"></script>
  <script src="{{ 'jquery-ui-slider-range.js' | asset_url }}"></script>
  <script src="{{ 'ajax-form.js' | asset_url }}"></script>
  <script src="{{ 'wow.min.js' | asset_url }}"></script>
  <script src="{{ 'imagesloaded.pkgd.min.js' | asset_url }}"></script>
  <script src="{{ 'main.js' | asset_url }}"></script>
  <script>

        $('#page-lost-password').hide();
        $('#move-to-lost').on('click',function() {
            $('#page-lost-password').show();
            $('#page-login').hide();
        })
        $('#back-login').on('click',function() {
            $('#page-lost-password').hide();
            $('#page-login').show();
        })
        
    
        </script>
</body>
</html>
